{
  "manifest_version": 2,
  "name": "Binance firefox plugins version 0.97" ,
  "version": "1.0",
  "description": "The plugin allows",
  "icons": {
    "48": "icons/48x48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["init.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/choose_page.html",
    "default_icon": {
      "16": "icons/favicon-16x16.png",
      "32": "icons/favicon-32x32.png"
    }
  },
  "permissions": [
    "notifications",
    "storage",
    "tabs"
  ]
}
